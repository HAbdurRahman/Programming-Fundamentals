#include<iostream>
using namespace std;

int main()
{
    int physics, chemistry, biology, maths, computer, obtained_marks,total_marks=500,percentage;
    cout << "Enter Marks in Physics" << endl;
    cin >> physics;
    cout << "Enter Marks in Chemistry" << endl;
    cin >> chemistry;
    cout << "Enter Marks in Biology" << endl;
    cin >> biology;
    cout << "Enter Marks in Maths" << endl;
    cin >> maths;
    cout << "Enter Marks in Computer" << endl;
    cin >> computer;
    obtained_marks = physics + chemistry + biology + maths + computer ;
     percentage =  (obtained_marks*100) /total_marks ;

    cout << "Percentage\t\t Grade" << endl;

    if (percentage >= 90 && percentage <= 100)
    {
        cout <<percentage <<"\t\t\t   "<< "A" << endl;
    }
    else if (percentage >= 80)
    {
        cout <<percentage <<"\t\t\t"<< "B" << endl;
    }
    else if (percentage >= 70)
    {
        cout <<percentage <<"\t\t\t"<< "C" << endl;
    }
    else if (percentage >= 60)
    {
        cout <<percentage <<"\t\t\t"<< "D" << endl;
    }
    else if (percentage >= 40)
    {
        cout <<percentage <<"\t\t\t"<< "E" << endl;
    }
    else if (percentage < 40)
    {
        cout <<percentage <<"\t\t\t"<< "F" << endl;
    }




    return 0;

}
