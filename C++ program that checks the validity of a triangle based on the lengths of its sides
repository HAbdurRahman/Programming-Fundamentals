#include<iostream>
using namespace std;

int main()
{
    int a,b,c;
    cout << "Enter 1st side of a Triangle" << endl;
    cin >> a;
    cout << "Enter 2nd side of a Triangle" << endl;
    cin >> b;
    cout << "Enter 3rd side of a Triangle" << endl;
    cin >> c;
    if (a+b>=c || b+c>=a || a+c>=b)
    {
        cout << "Triangle is Valid" << endl;
    }
    else
    {
        cout << "Triangle is Invalid" << endl;
    }

    return 0;

}
